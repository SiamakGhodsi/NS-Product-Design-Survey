	journal = {Procedia Computer Science},
	author = {Terziyan, Vagan and Vitko, Oleksandra},
	month = jan,
	year = {2024},
	keywords = {Industry 4.0, informed machine learning, machine learning, neural networks, physics-informed neural networks, taxonomy},
	pages = {1388--1399},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\ghodsis\\Zotero\\storage\\UG2X3BGZ\\Terziyan and Vitko - 2024 - Taxonomy-Informed Neural Networks for Smart Manufacturing.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\ghodsis\\Zotero\\storage\\3WCC22T3\\S1877050924001376.html:text/html},
}

@article{peres_industrial_2020,
	title = {Industrial {Artificial} {Intelligence} in {Industry} 4.0 - {Systematic} {Review}, {Challenges} and {Outlook}},
