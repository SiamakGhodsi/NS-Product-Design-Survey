	pages = {257--279},
	file = {Full Text PDF:C\:\\Users\\ghodsis\\Zotero\\storage\\IUJLKPHN\\Lu et al. - 2024 - Surveying neuro-symbolic approaches for reliable artificial intelligence of things.pdf:application/pdf},
