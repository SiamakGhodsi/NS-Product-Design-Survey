	file = {Full Text PDF:C\:\\Users\\ghodsis\\Zotero\\storage\\6CM6BN6X\\Amos and Kolter - 2017 - OptNet Differentiable Optimization as a Layer in Neural Networks.pdf:application/pdf},
}

