	file = {PDF:C\:\\Users\\ghodsis\\Zotero\\storage\\WVJS24M5\\Seff et al. - 2020 - SketchGraphs A Large-Scale Dataset for Modeling Relational Geometry in Computer-Aided Design.pdf:application/pdf},
}

